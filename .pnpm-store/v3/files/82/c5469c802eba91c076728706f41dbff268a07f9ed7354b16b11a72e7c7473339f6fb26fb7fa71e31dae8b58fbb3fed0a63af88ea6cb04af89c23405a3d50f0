/**
 * Job represents the configuration of a single job.
 */
export class IoK8sApiBatchV1Job extends Model {
    constructor(data) {
        super({
            apiVersion: IoK8sApiBatchV1Job.apiVersion,
            kind: IoK8sApiBatchV1Job.kind,
            ...data
        });
    }
}
IoK8sApiBatchV1Job.apiVersion = "batch/v1";
IoK8sApiBatchV1Job.kind = "Job";
Model.setSchema(IoK8sApiBatchV1Job, "io.k8s.api.batch.v1.Job", addSchema);
