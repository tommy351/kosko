import { Model } from "@kubernetes-models/base";
import { addSchema } from "../_schemas/IoK8sApiCoreV1SecretProjection.mjs";
/**
 * Adapts a secret into a projected volume.
 *
 * The contents of the target Secret's Data field will be presented in a projected volume as files using the keys in the Data field as the file names. Note that this is identical to a secret volume source without the default mode.
 */
export class IoK8sApiCoreV1SecretProjection extends Model {
}
Model.setSchema(IoK8sApiCoreV1SecretProjection, "io.k8s.api.core.v1.SecretProjection", addSchema);
export { IoK8sApiCoreV1SecretProjection as SecretProjection };
