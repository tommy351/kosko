import { Model } from "@kubernetes-models/base";
import { addSchema } from "../_schemas/IoK8sApiStorageV1CSIDriver.mjs";
/**
 * CSIDriver captures information about a Container Storage Interface (CSI) volume driver deployed on the cluster. Kubernetes attach detach controller uses this object to determine whether attach is required. Kubelet uses this object to determine whether pod information needs to be passed on mount. CSIDriver objects are non-namespaced.
 */
export class IoK8sApiStorageV1CSIDriver extends Model {
    constructor(data) {
        super({
            apiVersion: IoK8sApiStorageV1CSIDriver.apiVersion,
            kind: IoK8sApiStorageV1CSIDriver.kind,
            ...data
        });
    }
}
IoK8sApiStorageV1CSIDriver.apiVersion = "storage.k8s.io/v1";
IoK8sApiStorageV1CSIDriver.kind = "CSIDriver";
Model.setSchema(IoK8sApiStorageV1CSIDriver, "io.k8s.api.storage.v1.CSIDriver", addSchema);
export { IoK8sApiStorageV1CSIDriver as CSIDriver };
