import { register } from "@kubernetes-models/validate";
const schema = {
    "properties": {
        "path": {
            "type": "string"
        },
        "type": {
            "type": "string"
        }
    },
    "required": [
        "path"
    ],
    "type": "object"
};
export function addSchema() {
    register("io.k8s.api.core.v1.HostPathVolumeSource", schema);
}
