import Ajv from "ajv";
import { ajv } from "./ajv.mjs";
export function validate(id, data) {
    if (!ajv.validate(id, data) && ajv.errors) {
        const err = new Ajv.ValidationError(ajv.errors);
        err.message = ajv.errorsText(ajv.errors);
        throw err;
    }
}
