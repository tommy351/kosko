import { Model } from "@kubernetes-models/base";
import { addSchema } from "../_schemas/IoK8sApiNodeV1beta1RuntimeClass.mjs";
/**
 * RuntimeClass defines a class of container runtime supported in the cluster. The RuntimeClass is used to determine which container runtime is used to run all containers in a pod. RuntimeClasses are (currently) manually defined by a user or cluster provisioner, and referenced in the PodSpec. The Kubelet is responsible for resolving the RuntimeClassName reference before running the pod.  For more details, see https://git.k8s.io/enhancements/keps/sig-node/runtime-class.md
 */
export class IoK8sApiNodeV1beta1RuntimeClass extends Model {
    constructor(data) {
        super({
            apiVersion: IoK8sApiNodeV1beta1RuntimeClass.apiVersion,
            kind: IoK8sApiNodeV1beta1RuntimeClass.kind,
            ...data
        });
    }
}
IoK8sApiNodeV1beta1RuntimeClass.apiVersion = "node.k8s.io/v1beta1";
IoK8sApiNodeV1beta1RuntimeClass.kind = "RuntimeClass";
Model.setSchema(IoK8sApiNodeV1beta1RuntimeClass, "io.k8s.api.node.v1beta1.RuntimeClass", addSchema);
export { IoK8sApiNodeV1beta1RuntimeClass as RuntimeClass };
