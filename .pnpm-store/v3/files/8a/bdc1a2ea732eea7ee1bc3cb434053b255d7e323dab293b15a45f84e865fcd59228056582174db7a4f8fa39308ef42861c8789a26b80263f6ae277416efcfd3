import { Model } from "@kubernetes-models/base";
/**
 * SeccompProfile defines a pod/container's seccomp profile settings. Only one profile source may be set.
 */
export interface IIoK8sApiCoreV1SeccompProfile {
    /**
     * localhostProfile indicates a profile defined in a file on the node should be used. The profile must be preconfigured on the node to work. Must be a descending path, relative to the kubelet's configured seccomp profile location. Must only be set if type is "Localhost".
     */
    "localhostProfile"?: string;
    /**
     * type indicates which kind of seccomp profile will be applied. Valid options are:
     *
     * Localhost - a profile defined in a file on the node should be used. RuntimeDefault - the container runtime default profile should be used. Unconfined - no profile should be applied.
     */
    "type": string;
}
/**
 * SeccompProfile defines a pod/container's seccomp profile settings. Only one profile source may be set.
 */
export declare class IoK8sApiCoreV1SeccompProfile extends Model<IIoK8sApiCoreV1SeccompProfile> implements IIoK8sApiCoreV1SeccompProfile {
    "localhostProfile"?: string;
    "type": string;
}
export { IIoK8sApiCoreV1SeccompProfile as ISeccompProfile, IoK8sApiCoreV1SeccompProfile as SeccompProfile };
