import { register } from "@kubernetes-models/validate";
import { addSchema as IoK8sApiCoreV1PersistentVolumeSpec } from "./IoK8sApiCoreV1PersistentVolumeSpec.mjs";
const schema = {
    "properties": {
        "inlineVolumeSpec": {
            "$ref": "io.k8s.api.core.v1.PersistentVolumeSpec#"
        },
        "persistentVolumeName": {
            "type": "string"
        }
    },
    "type": "object"
};
export function addSchema() {
    IoK8sApiCoreV1PersistentVolumeSpec();
    register("io.k8s.api.storage.v1alpha1.VolumeAttachmentSource", schema);
}
