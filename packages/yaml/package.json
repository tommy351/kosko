{
  "name": "@kosko/yaml",
  "version": "3.1.0",
  "description": "Load Kubernetes YAML files in kosko.",
  "homepage": "https://kosko.dev",
  "repository": "https://github.com/tommy351/kosko/tree/master/packages/yaml",
  "author": "Tommy Chen <tommy351@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup"
  },
  "files": [
    "/dist/"
  ],
  "keywords": [
    "kubernetes",
    "kosko",
    "yaml"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.18.0"
  },
  "dependencies": {
    "@kosko/common-utils": "workspace:^",
    "@kosko/log": "workspace:^",
    "@kosko/require": "workspace:^",
    "fast-safe-stringify": "^2.1.1",
    "js-yaml": "^4.1.0",
    "node-fetch": "^2.6.6"
  },
  "devDependencies": {
    "@kosko/jest-preset": "workspace:^",
    "@kosko/test-utils": "workspace:^",
    "@kosko/tsup-config": "workspace:^",
    "@types/js-yaml": "^4.0.5",
    "@types/node-fetch": "^2.6.2",
    "execa": "^5.1.1",
    "fetch-mock": "^9.11.0",
    "kubernetes-models": "^4.1.0",
    "tsup": "^6.4.0"
  },
  "peerDependencies": {
    "kubernetes-models": ">=1.0.3"
  },
  "jest": {
    "preset": "@kosko/jest-preset"
  }
}
