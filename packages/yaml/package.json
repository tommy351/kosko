{
  "name": "@kosko/yaml",
  "version": "1.0.1",
  "description": "Load Kubernetes YAML files in kosko.",
  "homepage": "https://kosko.dev",
  "repository": "https://github.com/tommy351/kosko/tree/master/packages/yaml",
  "author": "Tommy Chen <tommy351@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "sideEffects": false,
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "files": [
    "/dist/"
  ],
  "keywords": [
    "kubernetes",
    "kosko",
    "yaml"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@kosko/require": "workspace:*",
    "debug": "^4.3.1",
    "fs-extra": "^9.1.0",
    "js-yaml": "^4.0.0",
    "node-fetch": "^2.6.1",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/fs-extra": "^9.0.7",
    "@types/js-yaml": "^4.0.0",
    "@types/node-fetch": "^2.5.7",
    "fetch-mock": "^9.11.0",
    "kubernetes-models": "^1.5.2",
    "temp-dir": "^2.0.0",
    "tmp-promise": "^3.0.2"
  },
  "peerDependencies": {
    "kubernetes-models": ">=1.0.3"
  }
}
