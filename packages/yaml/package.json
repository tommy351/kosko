{
  "name": "@kosko/yaml",
  "version": "4.0.0",
  "description": "Load Kubernetes YAML files in kosko.",
  "homepage": "https://kosko.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/tommy351/kosko.git",
    "directory": "packages/yaml"
  },
  "author": "Tommy Chen <tommy351@gmail.com>",
  "license": "MIT",
  "main": "dist/index.node.cjs",
  "module": "dist/index.node.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "build-scripts"
  },
  "exports": {
    "node": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.node.mjs"
      },
      "types": "./dist/index.d.ts",
      "require": "./dist/index.node.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.base.mjs"
    },
    "types": "./dist/index.d.ts",
    "require": "./dist/index.node.cjs"
  },
  "files": [
    "/dist/"
  ],
  "keywords": [
    "kubernetes",
    "kosko",
    "yaml"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@kosko/common-utils": "workspace:^",
    "@kosko/log": "workspace:^",
    "fast-safe-stringify": "^2.1.1",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@kosko/build-scripts": "workspace:^",
    "@kosko/jest-preset": "workspace:^",
    "@kosko/test-utils": "workspace:^",
    "@types/js-yaml": "^4.0.5",
    "@types/node-fetch": "^2.6.2",
    "cross-fetch": "^3.1.5",
    "execa": "^5.1.1",
    "fetch-mock": "^9.11.0",
    "kubernetes-models": "^4.1.0"
  },
  "peerDependencies": {
    "kubernetes-models": ">=1.0.3"
  },
  "optionalDependencies": {
    "node-fetch": "^2.6.6"
  },
  "jest": {
    "preset": "@kosko/jest-preset"
  }
}
