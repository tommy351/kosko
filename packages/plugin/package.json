{
  "name": "@kosko/plugin",
  "version": "0.3.0",
  "description": "Plugin types for Kosko.",
  "homepage": "https://kosko.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/tommy351/kosko.git",
    "directory": "packages/plugin"
  },
  "author": "Tommy Chen <tommy351@gmail.com>",
  "license": "MIT",
  "main": "dist/index.node.cjs",
  "module": "dist/index.node.mjs",
  "browser": "dist/index.base.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "build-scripts"
  },
  "exports": {
    "node": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.node.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.node.cjs"
      }
    },
    "types": "./dist/index.d.mts",
    "default": "./dist/index.base.mjs"
  },
  "files": [
    "/dist/"
  ],
  "keywords": [
    "kosko"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@kosko/generate": "workspace:^",
    "superstruct": "^1.0.3"
  },
  "devDependencies": {
    "@kosko/build-scripts": "workspace:^"
  }
}
