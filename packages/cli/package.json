{
  "name": "@kosko/cli",
  "version": "3.0.2",
  "description": "Organize Kubernetes manifests in JavaScript.",
  "homepage": "https://kosko.dev",
  "repository": "https://github.com/tommy351/kosko/tree/master/packages/cli",
  "author": "Tommy Chen <tommy351@gmail.com>",
  "license": "MIT",
  "bin": {
    "kosko": "bin/kosko.js"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./bin/kosko.js": "./bin/kosko.js"
  },
  "scripts": {
    "build": "tsup"
  },
  "files": [
    "/bin/",
    "/dist/",
    "/templates/"
  ],
  "keywords": [
    "kubernetes",
    "kosko"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.18.0"
  },
  "dependencies": {
    "@kosko/aggregate-error": "workspace:^",
    "@kosko/common-utils": "workspace:^",
    "@kosko/config": "workspace:^",
    "@kosko/exec-utils": "workspace:^",
    "@kosko/generate": "workspace:^",
    "@kosko/log": "workspace:^",
    "@kosko/migrate": "workspace:^",
    "@kosko/require": "workspace:^",
    "clean-stack": "^3.0.1",
    "exit": "^0.1.2",
    "extract-stack": "^2.0.0",
    "fast-safe-stringify": "^2.1.1",
    "get-stdin": "^8.0.0",
    "import-local": "^3.1.0",
    "jsonpath": "^1.1.1",
    "picocolors": "^1.0.0",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@iarna/toml": "^2.2.5",
    "@kosko/env": "workspace:^",
    "@kosko/jest-preset": "workspace:^",
    "@kosko/test-utils": "workspace:^",
    "@kosko/tsup-config": "workspace:^",
    "@types/bl": "^5.0.2",
    "@types/exit": "^0.1.31",
    "@types/jsonpath": "^0.2.0",
    "@types/yargs": "^17.0.12",
    "bl": "^5.0.0",
    "execa": "^5.1.1",
    "fast-glob": "^3.2.12",
    "kubernetes-models": "^4.1.0",
    "pkg-dir": "^5.0.0",
    "tsup": "^6.4.0"
  },
  "jest": {
    "preset": "@kosko/jest-preset"
  }
}
