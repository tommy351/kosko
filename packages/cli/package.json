{
  "name": "@kosko/cli",
  "version": "4.3.0",
  "description": "Organize Kubernetes manifests in TypeScript.",
  "homepage": "https://kosko.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/tommy351/kosko.git",
    "directory": "packages/cli"
  },
  "author": "Tommy Chen <tommy351@gmail.com>",
  "license": "MIT",
  "bin": {
    "kosko": "bin/kosko.js"
  },
  "main": "dist/index.node.cjs",
  "module": "dist/index.node.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "build-scripts index.ts worker-bin.ts --types dist/src"
  },
  "exports": {
    "./bin/kosko.js": "./bin/kosko.js",
    ".": {
      "node": {
        "import": {
          "types": "./dist/index.d.mts",
          "default": "./dist/index.node.mjs"
        },
        "require": {
          "types": "./dist/index.d.ts",
          "default": "./dist/index.node.cjs"
        }
      },
      "types": "./dist/index.d.mts",
      "default": "./dist/index.base.mjs"
    }
  },
  "files": [
    "/bin/",
    "/dist/"
  ],
  "keywords": [
    "kubernetes",
    "kosko"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@kosko/cli-utils": "workspace:^",
    "@kosko/common-utils": "workspace:^",
    "@kosko/config": "workspace:^",
    "@kosko/exec-utils": "workspace:^",
    "@kosko/generate": "workspace:^",
    "@kosko/log": "workspace:^",
    "@kosko/migrate": "workspace:^",
    "@kosko/require": "workspace:^",
    "clean-stack": "^3.0.1",
    "fast-safe-stringify": "^2.1.1",
    "get-stdin": "^8.0.0",
    "import-local": "^3.1.0",
    "jsonpath": "^1.1.1",
    "picocolors": "^1.0.0",
    "pkg-up": "^3.1.0",
    "superstruct": "^1.0.3",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@iarna/toml": "^2.2.5",
    "@kosko/build-scripts": "workspace:^",
    "@kosko/env": "workspace:^",
    "@kosko/jest-preset": "workspace:^",
    "@kosko/plugin": "workspace:^",
    "@kosko/test-utils": "workspace:^",
    "@types/bl": "^5.0.2",
    "@types/exit": "^0.1.31",
    "@types/jsonpath": "^0.2.0",
    "@types/yargs": "^17.0.12",
    "bl": "^5.0.0",
    "execa": "^5.1.1",
    "fast-glob": "^3.2.12",
    "kubernetes-models": "^4.1.0",
    "pkg-dir": "^5.0.0",
    "replace-string": "^3.1.0"
  },
  "jest": {
    "preset": "@kosko/jest-preset"
  }
}
