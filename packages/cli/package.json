{
  "name": "@kosko/cli",
  "version": "1.2.4",
  "description": "Organize Kubernetes manifests in JavaScript.",
  "homepage": "https://kosko.dev",
  "repository": "https://github.com/tommy351/kosko/tree/master/packages/cli",
  "author": "Tommy Chen <tommy351@gmail.com>",
  "license": "MIT",
  "bin": {
    "kosko": "bin/kosko.js"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./bin/kosko.js": "./bin/kosko.js"
  },
  "files": [
    "/bin/",
    "/dist/"
  ],
  "keywords": [
    "kubernetes",
    "kosko"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@kosko/config": "workspace:*",
    "@kosko/generate": "workspace:*",
    "@kosko/migrate": "workspace:*",
    "@kosko/require": "workspace:*",
    "chalk": "^4.1.0",
    "clean-stack": "^3.0.1",
    "debug": "^4.3.1",
    "exit": "^0.1.2",
    "fs-extra": "^9.1.0",
    "get-stdin": "^8.0.0",
    "import-local": "^3.0.2",
    "jsonpath": "^1.0.2",
    "signale": "^1.4.0",
    "tslib": "^2.1.0",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@iarna/toml": "^2.2.5",
    "@kosko/env": "workspace:*",
    "@types/debug": "^4.1.5",
    "@types/exit": "^0.1.31",
    "@types/fs-extra": "^9.0.7",
    "@types/jsonpath": "^0.2.0",
    "@types/signale": "^1.4.1",
    "@types/yargs": "^16.0.0",
    "pkg-dir": "^5.0.0",
    "temp-dir": "^2.0.0",
    "tmp-promise": "^3.0.2"
  }
}
