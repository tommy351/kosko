{
  "name": "@kosko/cli",
  "version": "3.1.0",
  "description": "Organize Kubernetes manifests in JavaScript.",
  "homepage": "https://kosko.dev",
  "repository": "https://github.com/tommy351/kosko/tree/master/packages/cli",
  "author": "Tommy Chen <tommy351@gmail.com>",
  "license": "MIT",
  "bin": {
    "kosko": "bin/kosko.js"
  },
  "main": "dist/index.node.cjs",
  "module": "dist/index.node.mjs",
  "types": "dist/types.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "build-scripts src/index.ts src/worker-bin.ts"
  },
  "exports": {
    "./bin/kosko.js": "./bin/kosko.js",
    ".": {
      "node": {
        "import": {
          "types": "./dist/types.d.mts",
          "default": "./dist/index.node.mjs"
        },
        "types": "./dist/types.d.ts",
        "require": "./dist/index.node.cjs"
      },
      "import": {
        "types": "./dist/types.d.mts",
        "default": "./dist/index.base.mjs"
      },
      "types": "./dist/types.d.ts",
      "require": "./dist/index.node.cjs"
    }
  },
  "files": [
    "/bin/",
    "/dist/",
    "/templates/"
  ],
  "keywords": [
    "kubernetes",
    "kosko"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.18.0"
  },
  "dependencies": {
    "@kosko/aggregate-error": "workspace:^",
    "@kosko/common-utils": "workspace:^",
    "@kosko/config": "workspace:^",
    "@kosko/exec-utils": "workspace:^",
    "@kosko/generate": "workspace:^",
    "@kosko/log": "workspace:^",
    "@kosko/migrate": "workspace:^",
    "@kosko/require": "workspace:^",
    "clean-stack": "^3.0.1",
    "extract-stack": "^2.0.0",
    "fast-safe-stringify": "^2.1.1",
    "get-stdin": "^8.0.0",
    "import-local": "^3.1.0",
    "jsonpath": "^1.1.1",
    "picocolors": "^1.0.0",
    "pkg-up": "^3.1.0",
    "resolve-from": "^5.0.0",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@iarna/toml": "^2.2.5",
    "@kosko/build-scripts": "workspace:^",
    "@kosko/env": "workspace:^",
    "@kosko/jest-preset": "workspace:^",
    "@kosko/test-utils": "workspace:^",
    "@types/bl": "^5.0.2",
    "@types/exit": "^0.1.31",
    "@types/js-yaml": "^4.0.5",
    "@types/jsonpath": "^0.2.0",
    "@types/yargs": "^17.0.12",
    "bl": "^5.0.0",
    "execa": "^5.1.1",
    "fast-glob": "^3.2.12",
    "js-yaml": "^4.1.0",
    "kubernetes-models": "^4.1.0",
    "pkg-dir": "^5.0.0",
    "replace-string": "^3.1.0"
  },
  "jest": {
    "preset": "@kosko/jest-preset"
  }
}
