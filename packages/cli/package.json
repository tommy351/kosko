{
  "name": "@kosko/cli",
  "version": "2.1.1",
  "description": "Organize Kubernetes manifests in JavaScript.",
  "homepage": "https://kosko.dev",
  "repository": "https://github.com/tommy351/kosko/tree/master/packages/cli",
  "author": "Tommy Chen <tommy351@gmail.com>",
  "license": "MIT",
  "bin": {
    "kosko": "bin/kosko.js"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./bin/kosko.js": "./bin/kosko.js"
  },
  "files": [
    "/bin/",
    "/dist/",
    "/templates/"
  ],
  "keywords": [
    "kubernetes",
    "kosko"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "@kosko/config": "workspace:^",
    "@kosko/exec-utils": "workspace:^",
    "@kosko/generate": "workspace:^",
    "@kosko/log": "workspace:^",
    "@kosko/migrate": "workspace:^",
    "@kosko/require": "workspace:^",
    "exit": "^0.1.2",
    "get-stdin": "^8.0.0",
    "import-local": "^3.0.3",
    "jsonpath": "^1.1.1",
    "picocolors": "^1.0.0",
    "tslib": "^2.3.1",
    "yargs": "^17.3.0"
  },
  "devDependencies": {
    "@iarna/toml": "^2.2.5",
    "@kosko/env": "workspace:^",
    "@kosko/jest-preset": "workspace:^",
    "@types/exit": "^0.1.31",
    "@types/jsonpath": "^0.2.0",
    "@types/yargs": "^17.0.7",
    "fast-glob": "^3.2.7",
    "pkg-dir": "^5.0.0",
    "temp-dir": "^2.0.0",
    "tmp-promise": "^3.0.3"
  },
  "jest": {
    "preset": "@kosko/jest-preset"
  }
}
