{
  "name": "@kosko/env",
  "version": "4.0.1",
  "description": "Manage environemnts in kosko.",
  "homepage": "https://kosko.dev",
  "repository": "https://github.com/tommy351/kosko/tree/master/packages/env",
  "author": "Tommy Chen <tommy351@gmail.com>",
  "license": "MIT",
  "main": "dist/index-node.js",
  "module": "dist/index-node-esm.mjs",
  "browser": "dist/index.mjs",
  "types": "dist/index-node.d.ts",
  "sideEffects": false,
  "exports": {
    "node": {
      "import": "./dist/index-node-esm.mjs",
      "require": "./dist/index-node.js"
    },
    "require": "./dist/index.js",
    "default": "./dist/index.mjs"
  },
  "scripts": {
    "build": "tsup"
  },
  "files": [
    "/dist/"
  ],
  "keywords": [
    "kosko"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.18.0"
  },
  "dependencies": {
    "@kosko/common-utils": "workspace:^",
    "@kosko/log": "workspace:^",
    "@kosko/require": "workspace:^",
    "deepmerge": "^4.2.2",
    "is-plain-object": "^5.0.0"
  },
  "jest": {
    "preset": "@kosko/jest-preset"
  },
  "devDependencies": {
    "@kosko/jest-preset": "workspace:^",
    "@kosko/tsup-config": "workspace:^",
    "tsup": "^6.4.0"
  }
}
