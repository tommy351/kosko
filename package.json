{
  "name": "@kosko/monorepo",
  "version": "0.0.1",
  "private": true,
  "homepage": "https://kosko.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/tommy351/kosko.git"
  },
  "author": "Tommy Chen <tommy351@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "npm-run-all lint:*",
    "lint:eslint": "eslint . --ext .js,.ts,.jsx,.tsx,.cjs.,mjs",
    "lint:syncpack": "syncpack list-mismatches",
    "build": "tsc-multi",
    "clean": "rm -rf packages/*/dist && npm run build -- --clean",
    "watch": "npm run build -- --watch",
    "test": "npm-run-all test:*",
    "test:unit": "cross-env ESM_IMPORT_DISABLED=true jest",
    "test:integration": "jest --config integration/jest.config.js",
    "test:browser": "jest --config integration/browser/jest.config.js",
    "preinstall": "npx only-allow pnpm"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,.cjs,.mjs}": "eslint --fix"
  },
  "engines": {
    "node": ">=12"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.2",
    "@changesets/cli": "^2.18.1",
    "@tsconfig/node12": "^1.0.9",
    "@types/expect-puppeteer": "^4.4.7",
    "@types/jest": "^27.0.3",
    "@types/jest-environment-puppeteer": "^4.4.1",
    "@types/node": "^16.11.12",
    "@types/puppeteer": "^5.4.4",
    "@types/tmp": "^0.2.2",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "execa": "^5.1.1",
    "expect-puppeteer": "^6.0.2",
    "husky": "^7.0.4",
    "jest": "^27.4.3",
    "jest-extended": "^1.2.0",
    "jest-puppeteer": "^6.0.2",
    "jest-serializer-path": "^0.1.15",
    "kubernetes-models": "^3.0.1",
    "lint-staged": "^12.1.2",
    "npm-run-all": "^4.1.5",
    "pkg-dir": "^5.0.0",
    "prettier": "^2.5.1",
    "puppeteer": "^12.0.1",
    "syncpack": "^5.8.15",
    "temp-dir": "^2.0.0",
    "tmp-promise": "^3.0.3",
    "ts-jest": "^27.1.1",
    "ts-node": "^10.4.0",
    "tsc-multi": "^0.5.0",
    "typescript": "^4.5.3",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "syncpack": {
    "peer": false,
    "source": [
      "package.json",
      "internal/*/package.json",
      "packages/*/package.json"
    ]
  },
  "jest": {
    "projects": [
      "<rootDir>/packages/*"
    ]
  }
}
