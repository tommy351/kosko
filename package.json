{
  "name": "@kosko/monorepo",
  "version": "0.0.1",
  "private": true,
  "homepage": "https://kosko.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/tommy351/kosko.git"
  },
  "author": "Tommy Chen <tommy351@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "npm-run-all lint:*",
    "lint:eslint": "eslint . --ext .js,.ts,.jsx,.tsx,.cjs.,mjs",
    "lint:syncpack": "syncpack list-mismatches --workspace false",
    "build": "turbo run build --filter=!./examples/* --filter=!./website",
    "clean": "rm -rf internal/*/dist packages/*/dist",
    "watch": "npm run build -- --watch",
    "test": "npm-run-all test:*",
    "test:unit": "cross-env ESM_IMPORT_DISABLED=true jest",
    "test:integration": "jest --config jest.integration.config.ts",
    "test:browser": "jest --config integration/browser/jest.config.js",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,.cjs,.mjs}": "eslint --fix"
  },
  "engines": {
    "node": ">=14.18.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.6",
    "@changesets/cli": "^2.24.4",
    "@tsconfig/node14": "^1.0.3",
    "@types/expect-puppeteer": "^5.0.1",
    "@types/jest": "^29.0.3",
    "@types/jest-environment-puppeteer": "^5.0.2",
    "@types/node": "^18.7.18",
    "@types/puppeteer": "^5.4.6",
    "@types/tmp": "^0.2.3",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.0.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-unicorn": "^44.0.2",
    "execa": "^5.1.1",
    "expect-puppeteer": "^6.1.1",
    "husky": "^8.0.0",
    "jest": "^29.0.3",
    "jest-extended": "^3.1.0",
    "jest-puppeteer": "^6.1.1",
    "jest-serializer-path": "^0.1.15",
    "kubernetes-models": "^4.1.0",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "pkg-dir": "^5.0.0",
    "prettier": "^2.7.1",
    "puppeteer": "^18.0.3",
    "read-pkg": "^5.2.0",
    "syncpack": "^8.2.4",
    "temp-dir": "^2.0.0",
    "tmp-promise": "^3.0.3",
    "ts-jest": "^29.0.1",
    "ts-node": "^10.9.1",
    "turbo": "^1.6.3",
    "typescript": "^4.8.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "syncpack": {
    "peer": false,
    "source": [
      "package.json",
      "internal/*/package.json",
      "packages/*/package.json"
    ]
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "react": "17",
        "react-dom": "17"
      }
    }
  }
}
